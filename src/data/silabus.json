{
  "curriculums": [
    {
      "id": "data-science-beginner",
      "title": "Data Science For Beginner",
      "icon": "BookOpen",
      "description": "Pelajari fondasi Data Science dari awal. Cocok untuk pemula yang ingin memulai karir di bidang data science dengan memahami konsep dasar hingga implementasi praktis.",
      "topics": [
        "Python Basics",
        "Data Cleaning",
        "Exploratory Data Analysis",
        "Basic Statistics",
        "Data Visualization"
      ],
      "modules": [
        {
          "week": 1,
          "title": "Pengenalan Python & Environment Setup",
          "topics": [
            "Installasi Python & Tools",
            "Jupyter Notebook Setup",
            "Basic Python Syntax",
            "Variables & Data Types"
          ]
        },
        {
          "week": 2,
          "title": "Data Structures & Libraries",
          "topics": [
            "List, Tuple, Dictionary",
            "Introduction to NumPy",
            "Introduction to Pandas",
            "Data Loading & Inspection"
          ]
        },
        {
          "week": 3,
          "title": "Data Cleaning & Preparation",
          "topics": [
            "Handling Missing Values",
            "Data Type Conversion",
            "Removing Duplicates",
            "Feature Scaling"
          ]
        },
        {
          "week": 4,
          "title": "Exploratory Data Analysis",
          "topics": [
            "Descriptive Statistics",
            "Data Visualization with Matplotlib",
            "Seaborn for Statistical Graphics",
            "Correlation Analysis"
          ]
        },
        {
          "week": 5,
          "title": "Basic Statistics & Probability",
          "topics": [
            "Distribution Analysis",
            "Hypothesis Testing",
            "Confidence Intervals",
            "P-values & Significance"
          ]
        }
      ]
    },
